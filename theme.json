{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#003C71",
					"name": "Primary Blue",
					"slug": "primary"
				},
				{
					"color": "#71003C",
					"name": "Secondary Burgundy",
					"slug": "secondary"
				},
				{
					"color": "#5B7553",
					"name": "Dark Green",
					"slug": "accent-dark"
				},
				{
					"color": "#8EB897",
					"name": "Light Green",
					"slug": "accent-light"
				},
				{
					"color": "#003c71",
					"name": "Main",
					"slug": "custom-main"
				},
				{
					"color": "#71003c",
					"name": "Secondary",
					"slug": "custom-secondary"
				},
				{
					"color": "#0057a4",
					"name": "Tertiary",
					"slug": "custom-tertiary"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "'Montserrat', sans-serif",
					"name": "Montserrat",
					"slug": "montserrat"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "clamp(0.875rem, 0.75rem + 0.625vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1rem, 0.875rem + 0.625vw, 1.25rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.25rem, 1rem + 1.25vw, 1.75rem)",
					"slug": "large"
				},
				{
					"name": "X-Large",
					"size": "clamp(1.5rem, 1.25rem + 1.25vw, 2.25rem)",
					"slug": "x-large"
				},
				{
					"name": "XX-Large",
					"size": "clamp(2rem, 1.5rem + 2.5vw, 3rem)",
					"slug": "xx-large"
				},
				{
					"name": "XXX-Large",
					"size": "clamp(2.5rem, 2rem + 2.5vw, 4rem)",
					"slug": "xxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--custom-secondary)",
					"text": "var(--wp--preset--color--white)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--white)"
						}
					}
				},
				"shadow": "var(--wp--preset--shadow--natural)",
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase"
				}
			},
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"textTransform": "uppercase"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-font)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}